    "@rsbuild/plugin-react": "1.4.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@cloudscape-design/components": "3.0.1128",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.66.0",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@1.22.22"
